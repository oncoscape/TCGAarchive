    "color":{ 
             "title": "color",
             "description": "used on validate color collections",
             "type": "object",
             "properties":{
                "dataset": {"type": "string"},
                "type": {"type": "string", "default": "color" },
                "name": {"type": "string"}, 
                "data": {"type": "array",
                         "items": {"type": "object",
                                   "properties":{
                                        "name": {"type": "string"},
                                        "color": {"pattern": "[A-Za-z0-9]+|^#[A-Z0-9]{6}"},    
                                        "values": {"type": "array",
                                                   "items":{
                                                            "type": "string", 
                                                            "pattern": "TCGA-[A-Z0-9]{2}-[A-Z0-9]{4}-[0-9]{2}"
                                                   }
                                                 } 
                                    }, 
                                    "required": ["name", "color", "values"]
                                  }
                        }
                 },
              "required": ["_id","dataset","type","name","data"],
              "additionalProperties": true  
              },
